    "url": "https://github.com/Kionling/portfolio-/issues"
  },
  "homepage": "https://github.com/Kionling/portfolio-#readme",
  "dependencies": {
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.2.0",
    "react-router-dom": "^6.10.0",
    "sequelize": "^6.30.0",
    "sequelize-cli": "^6.6.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "if-env": "^1.0.4",
    "nodemon": "^2.0.22"
  }
